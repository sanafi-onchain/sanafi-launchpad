# Environment Variables for Production Deployment
# Copy this file to .env.production and update with your production values

# Next.js Public Variables (exposed to browser) - PRODUCTION
NEXT_PUBLIC_POOL_CONFIG_KEY=production_pool_config_key_here
NEXT_PUBLIC_API_URL=https://api.your-domain.com
NEXT_PUBLIC_CHAIN_ID=101
NEXT_PUBLIC_ENVIRONMENT=production

# Server-only Variables (not exposed to browser) - PRODUCTION
DATABASE_URL=production_database_url
API_SECRET_KEY=production_secret_key
AWS_ACCESS_KEY_ID=production_aws_key
AWS_SECRET_ACCESS_KEY=production_aws_secret

# Add other production environment variables as needed
# CUSTOM_VAR=production_value
