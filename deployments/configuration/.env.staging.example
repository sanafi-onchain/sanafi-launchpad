# Environment Variables for Staging Deployment
# Copy this file to .env.staging and update with your staging values

# Next.js Public Variables (exposed to browser) - STAGING
NEXT_PUBLIC_POOL_CONFIG_KEY=staging_pool_config_key_here
NEXT_PUBLIC_API_URL=https://staging-api.your-domain.com
NEXT_PUBLIC_CHAIN_ID=101
NEXT_PUBLIC_ENVIRONMENT=staging

# Server-only Variables (not exposed to browser) - STAGING
DATABASE_URL=staging_database_url
API_SECRET_KEY=staging_secret_key
AWS_ACCESS_KEY_ID=staging_aws_key
AWS_SECRET_ACCESS_KEY=staging_aws_secret

# Add other staging environment variables as needed
# CUSTOM_VAR=staging_value
